<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>Vegiwoo.CSharpHelpers</PackageId>
    <Version>1.0.17</Version>
    <Authors>Vegi Woo</Authors>
    <Company>MPBoost Logistics</Company>
    <TargetFramework>net8.0</TargetFramework>
    <PlatformTarget>anycpu</PlatformTarget>
    <GenerateTargetFrameworkAttribute>false</GenerateTargetFrameworkAttribute>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild> 
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <UserSecretsId>bfed26e9-3d70-444b-aa5c-7e8c416f2f88</UserSecretsId>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="IronOCR" Version="2024.12.2" />
    <PackageReference Include="IronOcr.Languages.Russian" Version="2020.11.2" />
    <PackageReference Include="IronOcr.MacOs.ARM" Version="2024.12.2" />
    <PackageReference Include="itext" Version="9.0.0" />
    <PackageReference Include="itext.bouncy-castle-adapter" Version="9.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="9.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.UserSecrets" Version="9.0.0" />
  </ItemGroup>
</Project>
